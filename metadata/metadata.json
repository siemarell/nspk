{
    "cubes": [
        {
            "name": "Мониторинг серверов",
            "databaseName": "MyBase",
            "measureGroups": [
                {
                    "name": "Инциденты серверов",
                    "tableName": "f_serv_incident",
                    "dateColumn": "id_date_start",
                    "measures": [
                        {
                            "name": "Количество инцидентов",
                            "columnName": "fact_timedelta",
                            "aggregator": "COUNT"
                        },{
                            "name": "Длительность инцидентов",
                            "columnName": "fact_timedelta",
                            "aggregator": "SUM"
                        }

                    ],
                    "dimensionlinks": [
                        {
                            "dimensionId": 1,
                            "factColumnName": "id_date_start"
                        },
                        {
                            "dimensionId": 2,
                            "factColumnName": "id_date_start"
                        },
                        {
                            "dimensionId": 3,
                            "factColumnName": "id_time_start"
                        },
                        {
                            "dimensionId": 4,
                            "factColumnName": "id_host"
                        },
                        {
                            "dimensionId": 5,
                            "factColumnName": "id_trigger"
                        }
                    ]
                }
            ]
        },{
            "name": "Мониторинг каналов",
            "databaseName": "MyBase",
            "measureGroups": [
                {
                    "name": "Мониторинг каналов",
                    "tableName": "f_channel_connect",
                    "dateColumn": "id_date_start",
                    "measures": [
                        {
                            "name": "Количество инцидентов",
                            "columnName": "fact_timedelta",
                            "aggregator": "COUNT"
                        },{
                            "name": "Длительность инцидентов",
                            "columnName": "fact_timedelta",
                            "aggregator": "SUM"
                        },                        {
                            "name": "Количество инцидентов SLA",
                            "columnName": "fact_sla",
                            "aggregator": "COUNT"
                        },{
                            "name": "Длительность инцидентов SLA",
                            "columnName": "fact_sla",
                            "aggregator": "SUM"
                        }

                    ],
                    "dimensionlinks": [

                    ]
                }
            ]
        }
    ],
    "dimensions": [
         {
            "name": "Дата",
            "databaseName": "MyBase",
            "tableName": "calendar",
            "idColumnName": "date",
            "attributes": [
                {
                    "name": "Дата",
                    "columnName": "date"
                }
            ]
        },{
            "name": "Календарь",
            "databaseName": "MyBase",
            "tableName": "calendar",
            "idColumnName": "date",
            "attributes": [
                {
                    "name": "Год",
                    "columnName": "year"
                },
                {
                    "name": "Полугодие",
                    "columnName": "halfyear"
                },
                {
                    "name": "Квартал",
                    "columnName": "quarter"
                },
                {
                    "name": "Месяц",
                    "columnName": "month",
                    "orderByColumn": "month_number"
                },
                {
                    "name": "День",
                    "columnName": "date"
                }
            ]
        },{
            "name": "Время",
            "databaseName": "MyBase",
            "tableName": "d_time",
            "idColumnName": "id",
            "attributes": [
                {
                    "name": "Время",
                    "columnName": "time_text",
                    "orderBy": "id"
                }
            ]
        },{
            "name": "Хосты",
            "databaseName": "MyBase",
            "tableName": "d_host",
            "idColumnName": "id",
            "attributes": [
                {
                    "name": "Имя",
                    "columnName": "name"
                },{
                    "name": "Назначение",
                    "columnName": "purpose"
                },{
                    "name": "Подразделение-владелец",
                    "columnName": "division_owner"
                },{
                    "name": "Тип аппаратной платформы",
                    "columnName": "platform_type"
                },{
                    "name": "Операционная система",
                    "columnName": "os"
                },{
                    "name": "Администратор",
                    "columnName": "administrator"
                }
            ]
        },{
            "name": "Триггеры",
            "databaseName": "MyBase",
            "tableName": "d_trigger",
            "idColumnName": "id",
            "attributes": [
                {
                    "name": "Имя",
                    "columnName": "name"
                },{
                    "name": "Подсистема",
                    "columnName": "source_name"
                }
            ]
        }
    ]
}